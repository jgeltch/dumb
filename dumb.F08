PROGRAM DUMB

  IMPLICIT NONE
  INTEGER (KIND = 8) :: I, LIMIT

  INTEGER :: START_TIME, END_TIME, RATE

  CALL SYSTEM_CLOCK(START_TIME, RATE)

  DO CONCURRENT (I=0:2**32)
     IF (MOD(I, 1000000_8) == 0) THEN
        WRITE (*,*) I
     END IF
  END DO

  CALL SYSTEM_CLOCK(END_TIME)

  PRINT *, END_TIME - START_TIME
  
END PROGRAM DUMB
